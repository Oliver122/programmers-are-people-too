{
  "name": "programmersarepeopletoo",
  "displayName": "ProgrammersArePeopleToo",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "programmersarepeopletoo.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration":{
      "type":"object",
      "title": "Programmers are people too",
      "properties": {
        "ProgrammersArePeopleToo.enable":{
          "type": "boolean",
          "default": true,
          "description": "Extension acivation or deactivation"
        },
        "ProgrammersArePeopleToo.underline":{
          "type": "boolean",
          "default": true,
          "description": "Green underline under the code"
        },
        "ProgrammersArePeopleToo.animations":{
          "type": "boolean",
          "default": true,
          "description": "Animations in the line of code"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}


